{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
		"curly": false,
		"max-line-length": [false, 0],
		"indent": [true, "tabs", 2],
		"array-type": false,
		"align": false,
		"no-console": false,
		"max-classes-per-file": false,
		"member-ordering": false,
		"variable-name": false,
		"object-literal-sort-keys": false,
		"quotemark": false,
		"arrow-parens": false,
		"forin": false
	},
    "rulesDirectory": []
}